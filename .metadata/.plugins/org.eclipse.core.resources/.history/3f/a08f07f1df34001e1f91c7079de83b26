package com.pwright.rest.webservices.restfulwebservices.user;

import java.time.LocalDate;

public class User {
	
	private int id;
	private String name;
	private LocalDate birthDate;
	
	
	public User(int id, String name, LocalDate birthDate) {
		super();
		this.id = id;
		this.name = name;
		this.birthDate = birthDate;
	}
	
	
}
